{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "techfiedsolutions-adf"
		},
		"hospitalA_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hospitalA'"
		},
		"sqldataDataLakeLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sqldataDataLakeLinkedService'"
		},
		"hospitalA_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "techfiedsolutionsdev.database.windows.net"
		},
		"hospitalA_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "techfiedsolutions-hospital-a"
		},
		"hospitalA_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mikedev"
		},
		"sqldataDataLakeLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://techfiedsolutionadlsdev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "techfiedsolutions-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"hospitalA_password": {
						"value": "[parameters('hospitalA_password')]"
					},
					"sqldataDataLakeLinkedService_accountKey": {
						"value": "[parameters('sqldataDataLakeLinkedService_accountKey')]"
					},
					"hospitalA_properties_typeProperties_server": {
						"value": "[parameters('hospitalA_properties_typeProperties_server')]"
					},
					"hospitalA_properties_typeProperties_database": {
						"value": "[parameters('hospitalA_properties_typeProperties_database')]"
					},
					"hospitalA_properties_typeProperties_userName": {
						"value": "[parameters('hospitalA_properties_typeProperties_userName')]"
					},
					"sqldataDataLakeLinkedService_properties_typeProperties_url": {
						"value": "[parameters('sqldataDataLakeLinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}